<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/productfeed/:name" method="GET">
        <service class="MageApi\ProductFeed\Api\ProductFeedInterface" method="name"/>
        <resources>
            <resource ref="MageApi_ProductFeed::product"/>
        </resources>
    </route>
</routes>
